{
  "name": "cabojourney",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "prettier": "prettier 'src/**/*.{js,jsx}'",
    "lint-report": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o eslint_report.html",
    "test": "jest --silent --updateSnapshot --coverage --reporters default jest-stare",
    "test:silent": "jest --silent --coverage",
    "test:watch": "jest --watch",
    "test:file": "jest --silent",
    "verify": "yarn check --integrity",
    "code:check": "yarn verify",
    "prebuild:android": "yarn jetify && bundle install",
    "build:android": "bundle exec fastlane android build buildType:$buildType",
    "build": "yarn build",
    "prebundle:android": "yarn jetify && bundle install",
    "bundle:android": "bundle exec fastlane android bundle buildType:$buildType",
    "prepare": "patch-package",
    "local:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "local:androidclean": "cd android && ./gradlew clean && cd ..",
    "local:androidqaapk": "cd android && ./gradlew assembleDevRelease && cd ..",
    "local:run": "react-native run-android --variant=devDebug --port 8088",
    "local:start": "react-native start --port 8088",
    "husky-install": "husky install"
  },
